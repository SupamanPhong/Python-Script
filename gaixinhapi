import os
import json
from flask import Flask, request, jsonify
app = Flask(__name__)

@app.route('/')
def hello():
    return 'OK'

@app.route('/fimapi')
def summary():
    list = [
        {"url_image": "http://topanhdep.net/wp-content/uploads/2017/01/hinh-anh-tiep-vien-hang-khong-17.jpg"}
        
    ]
    return jsonify(results=list)
    
if __name__ == '__main__':    
    app.run(host=os.getenv('IP', '0.0.0.0'),port=int(os.getenv('PORT', 8080)))
